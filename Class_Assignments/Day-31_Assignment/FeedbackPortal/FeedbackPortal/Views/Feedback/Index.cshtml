@{
}
@model IEnumerable<FeedbackPortal.Models.Feedback>

<h2>Submitted Feedback</h2>
<a asp-action="Create">Add Feedback</a>
<ul>
    @foreach (var item in Model)
    {
        <li>@item.Name - @item.Email - Rating: @item.Rating - @item.Comments</li>
        <star-rating value="@item.Rating"></star-rating>
    }
    
</ul>

