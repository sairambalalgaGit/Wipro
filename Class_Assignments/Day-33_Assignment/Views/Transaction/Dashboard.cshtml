@{
}
@model IEnumerable<BankingMVC.Models.Transaction>

<h2>Transaction Dashboard</h2>
<table>
    <tr><th>ID</th><th>Owner</th><th>Amount</th><th>Type</th><th>Date</th></tr>
    @foreach (var t in Model)
    {
        <tr>
            <td>@t.Id</td>
            <td>@t.AccountOwner</td>
            <td>@t.Amount</td>
            <td>@t.Type</td>
            <td>@t.Date</td>
        </tr>
    }
</table>

